	<groupId>net.codeattic</groupId>
	<artifactId>webrocket</artifactId>
	<version>0.1.0</version>
	<packaging>jar</packaging>

	<description>WebRocket - Minimalistic web framework</description>
	<inceptionYear>2011</inceptionYear>

	<scm>
